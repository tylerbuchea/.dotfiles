# Go Lang
export GOPATH=$HOME/go/
PATH=$GOPATH/bin:$PATH

# Node.JS
#export NODE_PATH=$NODE_PATH:$HOME/.nvm/versions/node/v0.12.7/lib/node_modules

# Android SDK
export PATH=${PATH}:$HOME/Library/Android/sdk/platform-tools:$HOME/Library/Android/sdk/tools
export ANDROID_HOME=$HOME/Library/Android/sdk

# rbenv
export PATH=$HOME/.rbenv/bin:$PATH
eval "$(rbenv init -)"

# NVM
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh" # This loads nvm

# Editor Setting
export EDITOR=vim

# Python
export PATH="/usr/local/opt/python/libexec/bin:$PATH"

# added by Miniconda3 installer
export PATH="/Users/tylerbuchea/miniconda3/bin:$PATH"