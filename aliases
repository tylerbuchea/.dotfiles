# Git Commands
alias gpst="git subtree push --prefix dist origin dist"
alias gpgp="git subtree push --prefix dist origin gh-pages"
alias gds="git diff --stat"
alias gitwho="git shortlog -s -n"
alias prevtags="git tag --sort=taggerdate | tail -n3"

# General Commands
alias sourcez="source ~/.zshrc"
alias c="clear"
# alias lz='find . -maxdepth 1 -type d -mindepth 1 -exec du -hs {} \'

# Rhombus commands
alias webapp="gradle bootRun -Dspring.profiles.active=integration,remotedev,frontenddev"
